# Base de datos PostgreSQL
# Formato: postgres://usuario:password@host:puerto/database
DATABASE_URL=postgres://tu_usuario:tu_password@localhost:5432/db_finanzas

# Clave secreta para JWT (debe coincidir con el backend-auth)
# Puedes generar una con: openssl rand -base64 32
JWT_SECRET=tu_clave_secreta_segura_para_jwt_aqui

# Puerto del servidor
PORT=3002
